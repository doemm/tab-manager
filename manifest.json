{
  "name": "TabManager",
  "version": "1.0.0",
  "description": "An extension for killing and organizing tabs.",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/window_close16.png",
      "32": "images/window_close32.png",
      "48": "images/window_close48.png",
      "128": "images/window_close128.png"
    }
  },
  "icons": {
    "16": "images/window_close16.png",
    "32": "images/window_close32.png",
    "48": "images/window_close48.png",
    "128": "images/window_close128.png"
  },
  "manifest_version": 2
}
